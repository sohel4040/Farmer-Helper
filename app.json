{
  "expo": {
    "name": "farmerHelper",
    "slug": "farmerHelper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to tag scans with city information.",
        "NSCameraUsageDescription": "This app uses the camera to capture images for analysis."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCix8obuloxQipWpIiCWl-r8-yUj8KiHpE"
      },
      "bundleIdentifier": "com.anonymous.farmerHelper"
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCix8obuloxQipWpIiCWl-r8-yUj8KiHpE"
        }
      },
      "package": "com.anonymous.farmerHelper"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "caf2324a-7f2a-4aaf-bfbc-943d02eb5251"
      }
    }
  }
}
